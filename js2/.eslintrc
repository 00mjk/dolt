{
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "linebreak-style": [2, "unix"],
    "no-new-wrappers": 2,
    "no-var": 2,
    "quotes": [2, "single"],
    "radix": 2,
    "semi": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
