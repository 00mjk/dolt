alias Geo = import "../util/geotypes.noms"

struct Incident {
	Category: String
	Description: String
	Address: String
	Date: String
	Geoposition: Geo.Geoposition
}

struct SQuadTree {
	Nodes: List(Incident)
	Tiles: Map(String, SQuadTree)
	Depth: UInt8
	NumDescendents: UInt32
	Path: String
	Georectangle: Geo.Georectangle
}

using List(Incident)
