#!/usr/bin/expect

# start client
set timeout 2
spawn dolt sql-client --host=0.0.0.0 --port=$PORT
set client $spawn_id


# write to mysql
expect -i client -re "\rdolt_repo_.*> "
send -i client "select * from mysql.user;\r"
expect -i client "select * from mysql.user;\r"

expect -i first -re "\rdolt_repo_.*> "





send -i client "quit\r"
expect -i client "quit\r"

expect -i client eof